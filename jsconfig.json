{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/assets/js/components/*"],
            "@mixins/*": ["src/assets/js/components/mixins/*"],
            "@pages/*": ["src/assets/js/pages/*"],
            "@plugins/*": ["src/assets/js/plugins/*"],
            "@store/*": ["src/assets/js/store/*"],
            "@utils/*": ["src/assets/js/utils/*"],
            "@utils": ["src/assets/js/utils/index"]
        }
    },
    "include": [
        "./src/assets/**/*.js",
        "./src/assets/**/*.vue"
    ]
}
